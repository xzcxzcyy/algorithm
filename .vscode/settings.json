// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "files.associations": {
        "*.tcc": "cpp",
        "iostream": "cpp",
        "xlocale": "cpp",
        "ostream": "cpp",
        "ios": "cpp",
        "xiosbase": "cpp"
    },
    "clang.cxxflags": [
        "--target=x86_64-w64-windows-gnu",
        "-std=c++11"
    ],
    "clang.executable": "/usr/bin/clang"
}